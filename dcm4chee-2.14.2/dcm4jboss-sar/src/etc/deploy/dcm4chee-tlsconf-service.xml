<server>
  <classpath codebase="lib" archives="dcm4chee.jar"/>
  <mbean code="org.dcm4chex.archive.mbean.TLSConfigService"
    name="dcm4chee.archive:service=TLSConfig"
    xmbean-dd="xmdesc/dcm4chee-tlsconf-xmbean.xml">
